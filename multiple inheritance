#  класс работника
class Minus:
    def minus(self, num_1, num_2):
        print(num_1 - num_2)


#  класс студента
class Multiply:
    def multiply(self, num_1, num_2):
        print(num_1 * num_2)


class MinusMultiply(Minus, Multiply):
    def __init__(self, num_1, num_2):
        self.__num_1 = num_1
        self.__num_2 = num_2


# класс работающего студента
number = MinusMultiply(2,3)
number.minus(2,3)
number.multiply(2,3)
