#  класс работника
class Minus:
    def minus(self, num_1, num_2):
        print(num_1 - num_2)


#  класс студента
class Multiply:
    def multiply(self, num_1, num_2):
        print(num_1 * num_2)


class MinusMultiply(Minus, Multiply):
    pass


# класс работающего студента
number = MinusMultiply()
number.minus(2,3)
number.multiply(2,3)
